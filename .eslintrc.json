{
    "plugins": ["prettier"],
    "env": {
        "es6": true,
        "browser": true
    },
    "globals": {
        "require": true
    },
    "rules": {
        "prettier/prettier": "error"
    },
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2018,
        "requireConfigFile": false,
        "sourceType": "module"
    }
}
